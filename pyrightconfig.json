{
  "include": [
    "src/xboing/controllers",
    "src/xboing/engine",
    "src/xboing/game",
    "src/xboing/layout",
    "src/xboing/renderers",
    "src/xboing/ui",
    "src/xboing/utils",
    "src/xboing/__init__.py"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    ".venv",
    "build",
    "dist",
    "src/xboing/scripts"
  ],
  "typeCheckingMode": "strict",
  "reportUnknownMemberType": "information",
  "reportUnknownVariableType": "information",
  "reportMissingTypeStubs": "none",
  "pythonVersion": "3.10",
  "pythonPlatform": "Darwin",
  "strict": [
    "src/xboing"
  ],
  "executionEnvironments": [
    {
      "root": "src"
    }
  ]
}
